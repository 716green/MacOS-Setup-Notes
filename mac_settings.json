{
  "editor.tabSize": 2,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.shell.osx": "/bin/zsh",
  "terminal.integrated.fontFamily": "Fira Code, MesloLGS NF, Hack Nerd Font",
  "terminal.integrated.macOptionIsMeta": true,
  "terminal.integrated.scrollback": 2000,
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.rendererType": "dom",
  "terminal.integrated.letterSpacing": 1,
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.fontWeight": "500",
  "editor.suggestSelection": "first",
  "todo-tree.tree.showScanModeButton": false,
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]"
  ],
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "workbench.iconTheme": "vscode-icons",
  "go.toolsGopath": "/Users/bob/go/",
  "go.gopath": "/Users/bob/go/",
  "go.useLanguageServer": true,
  "go.formatTool": "gofmt",
  "workbench.colorTheme": "SpunBeat Violet Cream",
  "window.zoomLevel": 0.1,
  "html.format.wrapLineLength": 0,
  "html.format.contentUnformatted": "pre,code,textarea",
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingTemplateStringBraces": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingTemplateStringBraces": true,
  "template-string-converter.autoRemoveTemplateString": true,
  "template-string-converter.convertOutermostQuotes": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
  "typescript.format.insertSpaceBeforeFunctionParenthesis": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
  "typescript.preferences.quoteStyle": "single",
  "javascript.preferences.quoteStyle": "single",
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": true,
  "prettier.singleQuote": true,
  "vetur.experimental.templateInterpolationService": true,
  "emmet.includeLanguages": {
    "vue": "html",
    "vue-html": "html",
    "javascript": "javascript"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "prettier.vueIndentScriptAndStyle": true,
  "javascript.format.insertSpaceBeforeFunctionParenthesis": true
}
